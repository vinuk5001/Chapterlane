 <style>
    body {
    font-family: Arial, sans-serif;
}

/* Product Cart Styling */
.product-cart-wrap {
    transition: transform 0.3s ease, box-shadow 0.3s ease;
}

.product-cart-wrap:hover {
    transform: translateY(-10px);
    box-shadow: 0 8px 20px rgba(0, 0, 0, 0.1);
}

.product-img img {
    transition: transform 0.3s ease;
}

.product-img:hover img {
    transform: scale(1.1);
}

.action-btn {
    transition: background-color 0.3s ease;
}

.action-btn:hover {
    background-color: #f5a623;
}

.rating-stars i {
    color: #FFD700;
    margin-right: 2px;
}

/* Customize buttons */
.btn-success, .btn-danger, .btn-warning {
    border: none;
    color: #fff;
    padding: 5px 10px;
    font-size: 12px;
    border-radius: 3px;
    cursor: pointer;
}

/* Mobile Header and Search Styling */
.mobile-header-content-area {
    background-color: #fff;
    padding: 15px;
    border-bottom: 1px solid #eaeaea;
}

.mobile-search {
    display: flex;
    justify-content: center;
    margin-bottom: 15px;
}

.mobile-search input {
    width: 80%;
    padding: 10px;
    border: 1px solid #ccc;
    border-radius: 4px 0 0 4px;
    outline: none;
}

.mobile-search button {
    padding: 10px 15px;
    border: 1px solid #ccc;
    border-left: none;
    background-color: #5a5a5a;
    color: white;
    border-radius: 0 4px 4px 0;
    cursor: pointer;
}

/* Sidebar Categories Styling */
.sidebar-categories {
    width: 250px;
    position: fixed;
    left: 0;
    top: 0;
    bottom: 0;
    background-color: #f8f8f8;
    padding: 20px;
    box-shadow: 2px 0 5px rgba(0, 0, 0, 0.1);
    overflow-y: auto;
}

.sidebar-categories .main-categori-wrap {
    width: 100%;
}

.sidebar-categories .categori-button-active-2 {
    display: block;
    font-size: 18px;
    font-weight: bold;
    color: #333;
    margin-bottom: 20px;
}

.sidebar-categories .categori-dropdown-wrap {
    list-style: none;
    padding: 0;
    margin: 0;
}

.sidebar-categories .categori-dropdown-wrap ul {
    list-style: none;
    padding: 0;
    margin: 0;
}

.sidebar-categories .categori-dropdown-wrap li {
    margin-bottom: 10px;
}

.sidebar-categories .categori-dropdown-wrap li a {
    text-decoration: none;
    color: #333;
    display: flex;
    align-items: center;
}

.sidebar-categories .categori-dropdown-wrap li a:hover {
    color: #007bff;
}

.sidebar-categories .categori-dropdown-wrap li i {
    margin-right: 10px;
    font-size: 18px;
    color: #007bff;
}


</style>
        
</head>


